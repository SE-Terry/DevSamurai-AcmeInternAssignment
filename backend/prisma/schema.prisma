generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  passwordhash String
  createdat    DateTime @default(now())
  updatedat    DateTime @updatedAt
}

model chart_data {
  date      DateTime @id
  people    Int
  companies Int
}
